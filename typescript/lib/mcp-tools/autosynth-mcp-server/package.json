{
  "name": "@vibekit/autosynth-mcp-server",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "autosynth-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build",
    "build": "pnpm exec tsc",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "watch": "nodemon --watch 'src/**/*.ts' --exec 'tsx' src/index.ts",
    "inspect": "pnpm run build && npx -y @modelcontextprotocol/inspector node ./dist/index.js"
  },
  "keywords": [
    "mcp",
    "autosynth",
    "triggerx",
    "automation",
    "scheduling"
  ],
  "author": "VibeKit",
  "license": "MIT",
  "description": "AutoSynth MCP Server - Automated job scheduling with time, event, and condition triggers",
  "dependencies": {
    "@google-a2a/types": "workspace:*",
    "@modelcontextprotocol/sdk": "catalog:",
    "arbitrum-vibekit-core": "workspace:*",
    "cors": "^2.8.5",
    "dotenv": "catalog:",
    "ethers": "catalog:",
    "express": "catalog:",
    "nanoid": "^5.0.7",
    "node-cron": "^3.0.3",
    "sdk-triggerx": "^0.1.12",
    "viem": "catalog:",
    "zod": "catalog:"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "catalog:",
    "@types/node-cron": "^3.0.11",
    "nodemon": "^3.1.9",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
